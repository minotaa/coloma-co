[sub_resource type="GDScript" id="GDScript_fulsm"]
resource_name = "healing_potion"
script/source = "extends Control

var cost = 100
var item_id = 0

func _on_button_pressed() -> void:
	#print(\"uhhh... healing potion... bought?\")
	#print(Man.get_player().gold)
	if Man.get_player().gold >= cost:
		if Man.get_player().bag.list.size() < 3:
			Man.get_player().gold -= cost
			Man.get_player().bag.add_item(ItemStack.new(Items.get_by_id(0), 1))
			Toast.add(\"You purchased: \" + str(ItemStack.new(Items.get_by_id(0), 1)))
			Toast.add(\"-\" + str(cost) + \" Gold\")
		else:
			Toast.add(\"You don't have the inventory space!\")
	else:
		Toast.add(\"You can't afford this!\")
"

